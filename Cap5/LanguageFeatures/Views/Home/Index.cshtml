@model IEnumerable<string>
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Language Features</title>
</head>
<body>
    <ul>
        @{
            Console.WriteLine(Json.Serialize(Model)); // This line is for debugging purposes and will not render in the HTML output.)
        }
        @foreach (string s in Model)
        {
            <li>@s</li>
        }
    </ul>
</body>
</html>